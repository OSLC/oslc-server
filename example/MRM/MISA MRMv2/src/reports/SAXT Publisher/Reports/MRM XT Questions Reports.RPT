REPORT "SIAM Report"
DESCRIPT "Displays a report on SIAM diagram contents providing an accountability trace between organizaiton units and target groups"
ID 35942
{
	FONT "Font4" { NAME "Courier" HEIGHT 12 }
	SETTING { DECIMALSEPARATOR "." }
	SETTING { LISTSEPARATOR "," }
	SETTING { MEASUREMENT ENGLISH }
	SETTING { PAGESIZE -1", 0.00 }
	SETTING { HEADER 1 "SIAM Report" }
	SETTING { REPORTFORMAT 4 }
	TABULAR 1
	{
		SELECT "Name" LEGEND "SIAM Name" LEGENDFONT Font4
		WHERE Class = Diagram
		WHERE "Type Number" = 102
		ORDERBY "Name"
		JOIN
		WHERE RELATION = "contains"
		JOIN
		WHERE Class = Symbol
		JOIN
		WHERE RELATION = "defined by"
		JOIN
		SELECT "Name" LEGEND "Definition Name" LEGENDFONT Font4, "Type" LEGENDFONT Font4
		WHERE Class = Definition
	}
}
