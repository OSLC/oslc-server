REPORT "Services Report"
DESCRIPT "Lists services and their properties"
ID 28534
{
	SETTING { DECIMALSEPARATOR "." }
	SETTING { LISTSEPARATOR "," }
	SETTING { MEASUREMENT ENGLISH }
	SETTING { PAGESIZE -1", 0.00 }
	SETTING { REPORTFORMAT 4 }
	FIELD "Description <-- Description" { SOURCE PROPERTY "Description" LENGTH 4074 TYPE MEMO LEGEND "Description" }
	FIELD "Output <-- Output" { SOURCE PROPERTY "Output" LENGTH 80 TYPE CHARACTER LEGEND "Output" }
	FIELD "Program <-- Program" { SOURCE PROPERTY "Program" LENGTH 80 TYPE CHARACTER LEGEND "Program" }
	FIELD "Status <-- Status" { SOURCE PROPERTY "Status" LENGTH 8 TYPE CHARACTER LEGEND "Status" }
	FIELD "Service Type <-- Service Type" { SOURCE PROPERTY "Service Type" LENGTH 20 TYPE CHARACTER LEGEND "Service Type" }
	TABULAR 1
	{
		SELECT "Name" LEGENDFONT Font4, "Description <-- Description" LEGEND "Description" LEGENDFONT Font4, 
		       "Output <-- Output" LEGEND "Output" LEGENDFONT Font4, 
		       "Program <-- Program" LEGEND "Program" LEGENDFONT Font4, 
		       "Status <-- Status" LEGEND "Status" LEGENDFONT Font4, 
		       "Service Type <-- Service Type" LEGEND "Service Type" LEGENDFONT Font4
		WHERE Class = Definition
		WHERE "Type Number" = 142
		ORDERBY "Name"
	}
}
