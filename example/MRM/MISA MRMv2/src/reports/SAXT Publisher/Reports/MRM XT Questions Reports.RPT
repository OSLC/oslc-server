REPORT "Service Output to Target Group"
DESCRIPT "Describes the Output a Service provides to a Target Group"
ID 38394
{
	FONT "Font4" { NAME "Courier" HEIGHT 12 }
	SETTING { DECIMALSEPARATOR "." }
	SETTING { LISTSEPARATOR "," }
	SETTING { MEASUREMENT ENGLISH }
	SETTING { PAGESIZE -1", 0.00 }
	SETTING { HEADER 1 "Service Output to Target Group" }
	SETTING { REPORTFORMAT 4 }
	TABULAR 1
	{
		SELECT "Name" LEGEND "Service" LEGENDFONT Font4
		WHERE Class = Definition
		WHERE "Type Number" = 142
		ORDERBY "Name"
		JOIN
		WHERE RELATION = "produces"
		JOIN
		SELECT "Name" LEGEND "Output" LEGENDFONT Font4
		WHERE Class = Definition
		WHERE "Type Number" = 152
		JOIN
		WHERE RELATION = "creates"
		JOIN
		SELECT "Name" LEGEND " Service Value" LEGENDFONT Font4
		WHERE Class = Definition
		WHERE "Type Number" = 167
		JOIN
		WHERE RELATION = "realizes"
		JOIN
		SELECT "Name" LEGEND "Outcome"
		WHERE Class = Definition
		WHERE "Type Number" = 151
		JOIN
		WHERE RELATION = "experiencedBy"
		JOIN
		SELECT "Name" LEGEND "Target Group"
		WHERE Class = Definition
		WHERE "Type Number" = 155
	}
}
