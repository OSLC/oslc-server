<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY bpmn-soa "http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#" >
]>


<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#"
     xml:base="http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:bpmn-soa="http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about=""/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#does -->

    <owl:ObjectProperty rdf:about="#does">
        <rdfs:range rdf:resource="#Activity"/>
        <rdfs:domain rdf:resource="#Performer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#doneBy -->

    <owl:ObjectProperty rdf:about="#doneBy">
        <rdfs:domain rdf:resource="#Activity"/>
        <rdfs:range rdf:resource="#Performer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#has -->

    <owl:ObjectProperty rdf:about="#has">
        <rdfs:range rdf:resource="#Activity"/>
        <rdfs:domain rdf:resource="#Process"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#is -->

    <owl:ObjectProperty rdf:about="#is">
        <rdfs:range rdf:resource="#Activity"/>
        <rdfs:domain rdf:resource="#Process"/>
        <rdfs:domain rdf:resource="#Task"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#isElementIn -->

    <owl:ObjectProperty rdf:about="#isElementIn">
        <rdfs:domain rdf:resource="#Activity"/>
        <rdfs:range rdf:resource="#Process"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#originatedFrom -->

    <owl:ObjectProperty rdf:about="#originatedFrom">
        <rdfs:domain rdf:resource="#Activity"/>
        <rdfs:range rdf:resource="#Process"/>
        <rdfs:range rdf:resource="#Task"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Activity -->

    <owl:Class rdf:about="#Activity">
        <rdfs:subClassOf rdf:resource="#Element"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#doneBy"/>
                <owl:onClass rdf:resource="#Performer"/>
                <owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#originatedFrom"/>
                <owl:onClass rdf:resource="#Task"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#originatedFrom"/>
                <owl:onClass rdf:resource="#Process"/>
                <owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#originatedFrom"/>
                <owl:onClass rdf:resource="#Process"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#originatedFrom"/>
                <owl:onClass rdf:resource="#Task"/>
                <owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#doneBy"/>
                <owl:onClass rdf:resource="#Performer"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isElementIn"/>
                <owl:onClass rdf:resource="#Process"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Actor -->

    <owl:Class rdf:about="#Actor"/>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Choreography -->

    <owl:Class rdf:about="#Choreography">
        <rdfs:subClassOf rdf:resource="#Process"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Collaboration -->

    <owl:Class rdf:about="#Collaboration">
        <rdfs:subClassOf rdf:resource="#Process"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Element -->

    <owl:Class rdf:about="#Element"/>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Orchestration -->

    <owl:Class rdf:about="#Orchestration">
        <rdfs:subClassOf rdf:resource="#Process"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Performer -->

    <owl:Class rdf:about="#Performer">
        <rdfs:subClassOf rdf:resource="#Actor"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Process -->

    <owl:Class rdf:about="#Process">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has"/>
                <owl:onClass rdf:resource="#Activity"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2010/0/bpmn-soa.owl#Task -->

    <owl:Class rdf:about="#Task"/>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
</rdf:RDF>
