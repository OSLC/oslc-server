# baseURI: http://www.omg.org/bmms
# imports: http://open-services.net/ns/am
# imports: http://purl.org/dc/elements/1.1
# imports: http://purl.org/dc/terms/
# imports: http://spinrdf.org/spin
# imports: http://www.w3.org/2004/02/skos/core

@prefix :        <http://www.omg.org/bmms#> .
@prefix am:      <http://open-services.net/ns/am#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://www.omg.org/bmms>
      rdf:type owl:Ontology ;
      owl:imports <http://open-services.net/ns/am> , <http://purl.org/dc/elements/1.1> , <http://purl.org/dc/terms/> , <http://spinrdf.org/spin> , <http://www.w3.org/2004/02/skos/core> .

:AccountableElement
      rdfs:subClassOf :UtilityClasses ;
      dc:description "Represents any element an Organization Unit may be responsible for." ;
      dc:title "Accountable Element" .

:Assessment
      rdf:type owl:Class ;
      rdfs:subClassOf :MotivationElement ;
      dc:description "The judgment that an influencer affects the employment of means and/or the achievement of ends" ;
      dc:title "Assessment" .

:AssessmentCategory
      rdfs:subClassOf :Category .

:Asset
      rdf:type owl:Class ;
      rdfs:subClassOf :AccountableElement , :MotivationElement ;
      dc:description "Something of value owned by the enterprise" ;
      dc:title "Asset" .

:Assumption
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is taken for granted or without proof" .

:BusinessPolicy
      rdf:type owl:Class ;
      rdfs:subClassOf :Directive ;
      dc:description "A directive that is concerned with directly controlling, influencing, or regulating the actions of an enterprise and the people in it and that is not directly enforceable" ;
      dc:title "Business Policy" .

:BusinessProcess
      rdf:type owl:Class ;
      rdfs:subClassOf :AccountableElement , :MotivationElement ;
      dc:description "The concept ‘business process’ is adopted from the OMG’s Business Process Modeling and Notation (BPMN) specification. " ;
      dc:title "Business Process" .

:BusinessRule
      rdf:type owl:Class ;
      rdfs:subClassOf :Directive ;
      dc:description "A practicable directive" ;
      dc:title "Business Rule" .

:Category
      rdfs:subClassOf owl:Thing ;
      dc:description "A class or division of people or things regarded as having particular shared characteristics" ;
      dc:title "Category" .

:Competitor
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is a rival enterprise in a struggle for advantage over the subject enterprise." .

:CorporateValue
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is an ideal, custom, or institution that an enterprise promotes or agrees with. It may be positive or negative, depending on point of view." ;
      dc:title "Corporate Value" .

:CourseOfAction
      rdf:type owl:Class ;
      rdfs:subClassOf :Means ;
      dc:description "A means that is an approach or plan for configuring some aspect of the enterprise involving things, processes, locations, people, timing, or motivation undertaken to achieve ends" ;
      dc:title "Course of Action" .

:Customer
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is a role played by an individual or enterprise that has investigated, ordered, received, or paid for products or services from the subject enterprise." .

:Department
      rdfs:subClassOf :OrganizationCategory ;
      dc:description "" .

:DesiredResult
      rdf:type owl:Class ;
      rdfs:subClassOf :End ;
      dc:description "An end that is a state or target that the enterprise intends to maintain or sustain" ;
      dc:title "Desired Result" .

:Directive
      rdf:type owl:Class ;
      rdfs:subClassOf :MotivationElement ;
      dc:description "Something that defines or constrains some aspect of an enterprise" ;
      dc:title "Directive" .

:End  rdf:type owl:Class ;
      rdfs:subClassOf :EndOrMeans , :MotivationElement ;
      dc:description "Something that is to be accomplished" ;
      dc:title "End" .

:EndOrMeans
      rdfs:subClassOf :UtilityClass .

:Environment
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is the aggregate of surrounding conditions or Influencers affecting the existence or development of an enterprise." .

:ExplicitCorporateValue
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "A corporate value that is explicitly declared" ;
      dc:title "Explicit Corporate Value" .

:ExternalInfluencer
      rdf:type owl:Class ;
      rdfs:subClassOf :Influencer ;
      dc:description "An influencer outside an enterprise's organizational boundaries that can impact its employment of means or achievement of ends" ;
      dc:title "External Influencer" .

:FixedAsset
      rdfs:subClassOf :Thing ;
      dc:description "An asset that is maintained over time and reused" ;
      dc:title "Fixed Asset" .

:Goal
      rdf:type owl:Class ;
      rdfs:subClassOf :DesiredResult ;
      dc:description "An end that is a state or condition of the enterprise to be brought about or sustained through appropriate means" ;
      dc:title "Goal" .

:Habit
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is a customary practice or use" .

:ImplicitCorporateValue
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "A corporate value that is not explicitly declared but understood by some or all of the people in an enterprise" ;
      dc:title "Implicit Corporate Value" .

:Influencer
      rdf:type owl:Class ;
      rdfs:subClassOf :MotivationElement ;
      dc:description "Something that has the capability of producing an effect without apparent exertion of tangible force or direct exercise of command, and often without deliberate effort or intent" ;
      dc:title "Influencer" .

:InfluencerCategory
      rdfs:subClassOf :Category .

:InfluencingOrganization
      rdf:type owl:Class ;
      rdfs:subClassOf :MotivationElement ;
      dc:description "An organization that is external to the enterprise, and that influences that enterprise" ;
      dc:title "Influencing Organization" .

:Infrastructure
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is the basic physical and organizational structures and facilities needed for the operation of the enterprise" .

:InternalInfluencer
      rdf:type owl:Class ;
      rdfs:subClassOf :Influencer ;
      dc:description "An influencer from within an enterprise that can impact its employment of means or achievement of ends" ;
      dc:title "Internal Influencer" .

:Issue
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is a point in question or a matter that is in dispute as between contending partners" .

:Liability
      rdf:type owl:Class ;
      rdfs:subClassOf :AccountableElement , :MotivationElement ;
      dc:description "A reservation of some resource to meet commitments" ;
      dc:title "Liability" .

:ManagementPrerogative
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is a right or privilege exercised by virtue of ownership or position in an enterprise" ;
      dc:title "Management Prerogative" .

:Means
      rdf:type owl:Class ;
      rdfs:subClassOf :EndOrMeans , :MotivationElement ;
      dc:description "A device, capability, regime, technique, restriction, agency, instrument, or method that may be called upon, activated, or enforced to achieve ends" ;
      dc:title "Means" .

:Mission
      rdf:type owl:Class ;
      rdfs:subClassOf :Means ;
      dc:description "A means that is the ongoing operational activity of an enterprise." ;
      dc:title "Mission" .

:MotivationElement
      rdf:type owl:Class ;
      rdfs:subClassOf am:Resource ;
      dc:description "The superclass of all BMM model elements" ;
      dc:title "Motivational Element" .

:Objective
      rdf:type owl:Class ;
      rdfs:subClassOf :DesiredResult ;
      dc:description "A desired result that is a specific time-targeted, measurable, attainable target that an enterprise seeks to meet in order to achieve its goals." ;
      dc:title "Objective" .

:Offering
      rdfs:subClassOf :FixedAsset ;
      dc:description "A fixed asset that is a specification of a product or service that can be supplied by the enterprise Instances of offerings, such as finished goods, are a kind of resource." ;
      dc:title "Offering" .

:Opportunity
      rdfs:subClassOf :AssessmentCategory ;
      dc:description "Assessment that an influencer can have a favorable impact on the employment of means or achievement of ends" .

:OrganizationCategory
      rdfs:subClassOf :Category .

:OrganizationUnit
      rdf:type owl:Class ;
      rdfs:subClassOf :MotivationElement ;
      dc:description "Represents any recognized association of people in the context of the enterprise." ;
      dc:title "Organization Unit" .

:Partner
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is an enterprise that shares risks and profit with the subject enterprise (or is associated with the subject enterprise to share risks and profit) because this is mutually beneficial" .

:PotentialImpact
      rdf:type owl:Class ;
      rdfs:subClassOf :MotivationElement ;
      dc:description "Evaluation that quantifies or qualifies an assessment in specific terms, types, or dimensions." ;
      dc:title "Potential Impact" .

:PotentialReward
      rdf:type owl:Class ;
      rdfs:subClassOf :PotentialImpact ;
      dc:description "A potential impact that indicates the probability of gain." ;
      dc:title "Potential Reward" .

:Regulation
      rdf:type owl:Class ;
      rdfs:subClassOf :Influencer , :InfluencerCategory ;
      dc:description "An influencer that is an order prescribed by an authority such as a government body or the management of an enterprise" , "An influencer that is an order prescribed by an authority such as a government body or the management of an enterprise." ;
      dc:title "Regulation" .

:Resource
      rdf:type owl:Class ;
      rdfs:subClassOf :Asset ;
      dc:description "An asset that is consumed in the operations of the enterprise and replenished." ;
      dc:title "Resource" .

:ResourceQuality
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is the quality or availability of resources" ;
      dc:title "Resource Quality" .

:Risk
      rdf:type owl:Class ;
      rdfs:subClassOf :PotentialImpact ;
      dc:description "A potential impact that indicates the possibility of loss, injury, disadvantage, or destruction the possibility of loss, injury, disadvantage, or destruction." ;
      dc:title "Risk" .

:Strategy
      rdfs:subClassOf :CourseOfAction ;
      dc:description "A course of action that is an element of a plan devised through the science and art of business leadership exercised to ensure the most advantageous conditions." ;
      dc:title "Strategy" .

:Strength
      rdfs:subClassOf :AssessmentCategory ;
      dc:description "Assessment that an influencer indicates an advantage or area of excellence within an enterprise that can impact its employment of means or achievement of ends" .

:Supplier
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is a role played by an individual or enterprise that can furnish or provide products or services to the subject enterprise" .

:Tactic
      rdfs:subClassOf :CourseOfAction ;
      dc:description "A course of action that is a device or expedient to be employed as part of a strategy." ;
      dc:title "Tactic" .

:Technology
      rdfs:subClassOf :InfluencerCategory ;
      dc:description "An influencer that is caused by developments in and limitations of technology the application of scientific knowledge for practical purposes." .

:Threat
      rdfs:subClassOf :AssessmentCategory ;
      dc:description "Assessment that an influencer can have an unfavorable impact on the enterprise’s employment of means or achievement of ends" .

:UtilityClasses
      rdfs:subClassOf owl:Thing ;
      dc:description "Subclasses introduced to keep the ontology simple while avoiding unintended inheritance due to property domains." ;
      dc:title "Utility Classes" .

:Vision
      rdf:type owl:Class ;
      rdfs:subClassOf :End ;
      dc:description "An end that is about the future state of the enterprise, without regard to how it is to be achieved." ;
      dc:title "Vision" .

:Weakness
      rdfs:subClassOf :AssessmentCategory ;
      dc:description "Assessment that an influencer indicates an area of inadequacy within an enterprise that can impact its employment of means or achievement of ends" .

:actsAs
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Directive , :OrganizationUnit ;
      rdfs:range :InfluencingOrganization , :Regulation ;
      dc:title "acts as" ;
      owl:inverseOf :isARolePlayedBy .

:affectsAchievementOf
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Assessment ;
      rdfs:range :End ;
      dc:title "affects achievement of" ;
      owl:inverseOf :hasAchievementAffectedBy .

:affectsEmploymentOf
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Assessment ;
      rdfs:range :Means ;
      dc:title "affects employment of" ;
      owl:inverseOf :hasEmploymentAffectedBy .

:amplifies
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Goal ;
      rdfs:range :Vision ;
      dc:title "amplifies" ;
      owl:inverseOf :isAmplifiedBy .

:channelsEffortsTowards
      rdf:type owl:ObjectProperty ;
      rdfs:domain :CourseOfAction ;
      rdfs:range :DesiredResult ;
      dc:title "channels efforts towards" ;
      owl:inverseOf :isSupportedBy .

:defines
      rdf:type owl:ObjectProperty ;
      rdfs:domain :OrganizationUnit ;
      rdfs:range :End ;
      dc:title "defines" ;
      owl:inverseOf :isDefinedBy .

:delivers
      rdf:type owl:ObjectProperty ;
      rdfs:domain :BusinessProcess ;
      rdfs:range :Offering ;
      dc:title "delivers" ;
      owl:inverseOf :isDeliveredBy .

:deploys
      rdf:type owl:ObjectProperty ;
      rdfs:domain :CourseOfAction ;
      rdfs:range :Asset ;
      dc:title "deploys" ;
      owl:inverseOf :isDeployedBy .

:description
      owl:sameAs dc:description .

:determines
      rdf:type owl:ObjectProperty ;
      rdfs:domain :OrganizationUnit ;
      rdfs:range :Strategy ;
      dc:title "determines" ;
      owl:inverseOf :isDeterminedBy .

:discharges
      rdf:type owl:ObjectProperty ;
      rdfs:domain :CourseOfAction ;
      rdfs:range :Liability ;
      dc:title "discharges" ;
      owl:inverseOf :isDischargedBy .

:effectsEnforcementLevelOf
      rdf:type owl:ObjectProperty ;
      dc:title "effects enforcement level of"^^xsd:string ;
      owl:inverseOf :hasEnforcementLevelEffectedBy .

:enables
      rdf:type owl:ObjectProperty ;
      rdfs:domain :CourseOfAction ;
      rdfs:range :CourseOfAction ;
      dc:title "enables" ;
      owl:inverseOf :isEnabledBy .

:establishes
      rdf:type owl:ObjectProperty ;
      rdfs:domain :OrganizationUnit ;
      rdfs:range :Means ;
      dc:title "establishes" ;
      owl:inverseOf :isEstablishedBy .

:governs
      rdf:type owl:ObjectProperty ;
      rdfs:domain :BusinessPolicy ;
      rdfs:range :BusinessProcess ;
      dc:title "governs" ;
      owl:inverseOf :isGovernedBy .

:governsAction
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Directive ;
      rdfs:range :CourseOfAction ;
      dc:title "governs actuib" ;
      owl:inverseOf :isGovernedByDirective .

:governsUseOf
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Directive ;
      rdfs:range :Asset ;
      dc:title "governs use of" ;
      owl:inverseOf :hasUseGovernedBy .

:guides
      rdf:type owl:ObjectProperty ;
      rdfs:domain :BusinessRule ;
      rdfs:range :BusinessProcess ;
      dc:title "guides" ;
      owl:inverseOf :isGuidedBy .

:hasEffectOn
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Influencer ;
      rdfs:range :EndOrMeans ;
      dc:title "has effect on" ;
      owl:inverseOf :isAffectedBy .

:hasEnforcementLevelEffectedBy
      rdf:type owl:ObjectProperty ;
      dc:title "has enforcement level effected by"^^xsd:string .

:identifies
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Assessment ;
      rdfs:range :PotentialImpact ;
      dc:title "identifies" ;
      owl:inverseOf :isSignificantTo .

:implements
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Tactic ;
      rdfs:range :Strategy ;
      dc:title "implements" ;
      owl:inverseOf :isImplementedBy .

:includesCourseOfAction
      rdf:type owl:ObjectProperty ;
      rdfs:domain :CourseOfAction ;
      rdfs:range :CourseOfAction ;
      dc:title "includes more specific course of action" ;
      owl:inverseOf :isIncludedInCourseOfAction .

:includesDesiredResult
      rdf:type owl:ObjectProperty ;
      rdfs:domain :DesiredResult ;
      rdfs:range :DesiredResult ;
      dc:title "includes more specific desired result" ;
      owl:inverseOf :isIncludedInDesiredResult .

:isBasisFor
      rdf:type owl:ObjectProperty ;
      rdfs:domain :BusinessPolicy ;
      rdfs:range :BusinessRule ;
      dc:title "is basis for" ;
      owl:inverseOf :isDerivedFrom .

:isFormulatedBasedOn
      rdf:type owl:ObjectProperty ;
      rdfs:domain :CourseOfAction ;
      rdfs:range :Directive ;
      dc:title "is formulated based on" ;
      owl:inverseOf :isSourceOf .

:isImpactedBy
      rdf:type owl:ObjectProperty ;
      rdfs:domain :End , :Means ;
      rdfs:range :Influencer ;
      dc:title "is impacted by" ;
      owl:inverseOf :impacts .

:isJudgmentOf
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Assessment ;
      rdfs:range :Influencer ;
      dc:title "is judgment of of" ;
      owl:inverseOf :isJudgedIn .

:isPlannedByMeansOf
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Mission ;
      rdfs:range :Strategy ;
      dc:title "quantifies" ;
      owl:inverseOf :isAComponentOfThePlanFor .

:isResponsibleFor
      rdf:type owl:ObjectProperty ;
      rdfs:domain :OrganizationUnit ;
      rdfs:range :AccountableElement ;
      dc:title "is responsible for" ;
      owl:inverseOf :isResponsibilityOf .

:isSourceOf
      rdf:type owl:ObjectProperty ;
      rdfs:domain :InfluencingOrganization ;
      rdfs:range :Influencer ;
      dc:title "is source of" ;
      owl:inverseOf :isFrom .

:makes
      rdf:type owl:ObjectProperty ;
      rdfs:domain :OrganizationUnit ;
      rdfs:range :Assessment ;
      dc:title "makes" ;
      owl:inverseOf :isMadeBy .

:makesOperative
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Mission ;
      rdfs:range :Vision ;
      dc:title "makes operative" ;
      owl:inverseOf :isMadeOperativeBy .

:manages
      rdf:type owl:ObjectProperty ;
      rdfs:domain :BusinessProcess ;
      rdfs:range :Asset ;
      dc:title "manages" ;
      owl:inverseOf :isManagedBy .

:name
      owl:sameAs dc:title .

:provides
      rdf:type owl:ObjectProperty ;
      rdfs:domain :FixedAsset ;
      rdfs:range :Resource ;
      dc:title "provides" ;
      owl:inverseOf :isProvidedBy .

:providesImpetusFor
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Assessment , :PotentialImpact ;
      rdfs:range :Directive ;
      dc:title "provides impetus for" ;
      owl:inverseOf :isMotivatedBy .

:quantifies
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Objective ;
      rdfs:range :Goal ;
      dc:title "quantifies" ;
      owl:inverseOf :isQuantifiedBy .

:realizes
      rdf:type owl:ObjectProperty ;
      rdfs:domain :BusinessProcess ;
      rdfs:range :CourseOfAction ;
      dc:title "realizes" ;
      owl:inverseOf :isRealizedBy .

:recognizes
      rdf:type owl:ObjectProperty ;
      rdfs:domain :OrganizationUnit ;
      rdfs:range :Influencer ;
      dc:title "recognizes" ;
      owl:inverseOf :isRecognizedBy .

:requires
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Offering ;
      rdfs:range :Resource ;
      dc:title "requires" ;
      owl:inverseOf :isRequiredBy .

:supportsAchievementOf
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Directive ;
      rdfs:range :DesiredResult ;
      dc:title "supports achievement of" ;
      owl:inverseOf :hasAchievementSupportedBy .

:usesFixed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Offering ;
      rdfs:range :Asset ;
      dc:title "uses fixed" ;
      owl:inverseOf :isUsedByOffering .
